<h1>Listing wishlists</h1>
<p id="notice"><%= notice %></p>

<table class="large-12">
  <thead>
    <tr>
      <th>Title</th>
      <th>Username</th>
    </tr>
  </thead>

  <tbody>
    <% @wishlists.each do |wishlist| %>
      <tr>
        <td><%= link_to wishlist.title, wishlist %></td>
        <td><%= wishlist.user.user_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wishlist', new_wishlist_path %>
